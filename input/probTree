import arith
import event

typeSystem probTree =
  imports arith, eventT
  types  _:label, _:tree
  order event < label

conSpec probTreeG:probTree =
  imports arithG, event
  modes connection
  constructors
    notLabel: [label] -> label -- iconic,
    construct: [label, numExp] -> tree -- emergent,
    addBranch: [tree, label, numExp] -> tree -- spatial-index,
    resolve: [tree, tree] -> tree -- search
