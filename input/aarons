typeSystem aarons =
    types _:a1, _:a2
    order

conSpec aaronsG:aarons =
  constructors
    c1 : [a1] -> a2,
    c2 : [a1,a1] -> a1,
    c3 : [a1,a2] -> a2,
    c4 : [a1,a2] -> a1

construction aarons:aaronsG =
    t:x:a2 <- c3[t1:x1:a1 <- c2[t11:x11:a1 <- c4[t11:x11:a1,
                                                 t:x:a2],
                                t11:x11:a1],
                t2:x2:a2 <- c1[t11:x11:a1]]
