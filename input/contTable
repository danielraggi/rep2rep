import arith
import event

typeSystem contTable =
    imports arith, eventT
    types _:name, _:oneDim, _:twoDim, _:table
    order oneDim < table, twoDim < table, event < name

conSpec contTableG:contTable =
    imports arithG, event
    modes sentential, grid
    constructors
        notName : [name] -> name -- iconic,
        buildOne : [name,numExp] -> oneDim -- spatial-index,
        buildTwo : [oneDim,oneDim,numExp] -> twoDim -- spatial-index,
        combine : [table,table] -> table -- notational-index
